<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tumblr_o3_1280"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageViewEnemy"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/rocket"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="117dp" />

    <TextView
        android:id="@+id/timerText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageViewPlayer"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/arw03up"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="205dp" />

    <TextView
        android:id="@+id/textViewtouch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageViewGif"
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="498dp" />

</androidx.constraintlayout.widget.ConstraintLayout>